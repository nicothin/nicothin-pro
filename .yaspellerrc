{
  "excludeFiles": [
    ".git",
    ".sass-cache",
    "_data",
    "_includes",
    "_layout",
    "_sass",
    "_site",
    "assets",
    "yaspeller",
    "node_modules",
    "libs"
  ],
  "ignoreCapitalization": true,
  "ignoreDigits": true,
  "ignoreUppercase": true,
  "ignoreUrls": true,
  "ignoreText": [
    ["---.+---", "gms"],
    ["^\\[\\!\\[.*$", "gm"],
    ["{%.+%}", "gm"]
  ],
  "lang": "ru,en",
  "fileExtensions": [
    ".md",
    ".html"
  ],
  "dictionary": [
    "nicothin",
    "vimrc",
    "subl",
    "WSL",
    "nodeJS",
    "github",
    "SCSS",
    "SASS",
    "img",
    "SVG",
    "postCSS",
    "JS",
    "inputrc",
    "dircolors",
    "(c|C)mder",
    "cmd",
    "cp",
    "ls",
    "mkdir",
    "mv",
    "rm",
    "OSX",
    "ST3",
    "(g|G)it(b|B)ash",
    "PowerShell",
    "bashrc",
    "winbuntu",
    "(в|В)(к|К)онтакт*",
    "(э|Э)лектромыл*",
    "глюк*",
    "радиокноп*",
    "автосохранени*",
    "автодополнени*",
    "скринкаст*",
    "постпроцессинг*",
